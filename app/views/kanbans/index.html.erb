<div class="container">
    <h1>Listing Kanbans</h1>

    <table id="movies">
      <tr>
        <th>Title</th>
        <th>Summary</th>
        <th>Columnholder</th>
      </tr>

    <% @kanbans.each do |kanb| %>
      <tr>
        <td><%= link_to kanb.name, kanban_path(kanb) %></td>
        <td><%= kanb.settings %></td>
        <td><%= kanb.columnholder %></td>
      </tr>
    <% end %>
    </table>

</div>
